{
	"name": "Use HTTPS2",
	"version": "0.0.1",
	"description": "Forces defined sites to use https instead of http",
	"homepage_url": "https://github.com/astronouth7303/use-https",
	"icons": { "16": "lock-16.png",
	           "32": "lock-32.png",
	           "48": "lock-48.png",
	           "64": "lock-64.png",
	          "128": "lock-128.png" },
	"page_action": {"default_icon": "lock-16.png",
					"default_title": "Use HTTPS",
					"default_popup": "options.html" },
	"permissions": ["tabs",
					"webRequest",
					"webRequestBlocking",
					"<all_urls>"
					],
	"background_page": "background.html",
	"options_page": "options.html"
}
